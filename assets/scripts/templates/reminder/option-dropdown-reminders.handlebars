<div id="tag-reminder-to-communication-select" class="tag-select-container">
  <label>Reminder Name Selection:</label>
  <select id="select-option-reminder-category" class="form-control select-option-value">
      <option class="form-control blank-option" placeholder="Blank" type="text" value="0"></option>
      {{#each reminders as |reminder|}}
        <option class="form-control option-company-name" name="reminder[reminder_ref_name]" placeholder="reminder Type" type="text" data-company-id="{{company.id}}" data-company-name="{{company.name}}" value="{{reminder.id}}">{{reminder.full_name}}</option>
      {{/each}}
  </select>
</div>
