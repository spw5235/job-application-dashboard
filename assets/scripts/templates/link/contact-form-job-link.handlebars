<div class="select-container">
  <select class="select-element">
    <option class="form-control blank-option" placeholder="Blank" type="text" value="-1">Click to Select</option>
    {{#each contacts as |contact|}}
      <option class="form-control option-element" name="contact[job_ref_text]" type="text" data-form-category-id="{{contact.id}}" data-list-ref-text="{{contact.job_ref_text}}" data-list-ref-id="{{contact.job_ref_id}}" value="{{contact.job_ref_id}}">{{contact.job_ref_text}}</option>
    {{/each}}
</select>
</div>
