<div id="tag-contact-to-communication-select" class="tag-select-container">
  <label>Contact Name Selection:</label>
  <select id="select-option-contact-category" class="form-control select-option-value">
      <option class="form-control blank-option" placeholder="Blank" type="text" value="0"></option>
      {{#each contacts as |contact|}}
        <option class="form-control option-company-name" name="contact[contact_ref_name]" placeholder="reminder Type" type="text" data-company-id="{{company.id}}" data-company-name="{{company.name}}" value="{{contact.id}}">{{contact.full_name}}</option>
      {{/each}}
  </select>
</div>
