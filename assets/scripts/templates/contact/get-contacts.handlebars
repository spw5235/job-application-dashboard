<h1>Contact Dashboard</h1>

<table class="table contact-summary-table table-hover">
  <thead>
    <tr>
      <th>Date</th>
      <th>Contact Subject</th>
      <th>Contact Url</th>
      <th>View Record</th>
    </tr>
  </thead>
  <tbody>
    {{#each contacts as |contact|}}
      <tr>
        <td class="display-contact-date">
          {{contact.docdate}}
        </td>
        <td class="display-contact-subject">
          {{contact.docsubject}}
        </td>
        <td class="display-contact-url">
          {{contact.docurl}}
        </td>
        <td class="display-contact-submit">
          <button type="button" class="btn btn-info btn-sm dashboard-contact-record-btn" data-current-contact-id="{{contact.id}}">View contact</button>
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>
<br><br>
<div class="dashboard-container">
  <button id="dashboard-new-job-btn" type="button" class="btn btn-success">Create Job</button>
</div>
